# ShieldEye Example Environment

# Database Configuration
DB_HOST=postgres
DB_PORT=55432
DB_NAME=shieldeye
DB_USER=shieldeye
DB_PASSWORD=CHANGE_ME_DB_PASSWORD

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=

# MinIO Configuration
MINIO_ENDPOINT=minio:9000
MINIO_ACCESS_KEY=CHANGE_ME_MINIO_ACCESS_KEY
MINIO_SECRET_KEY=CHANGE_ME_MINIO_SECRET_KEY

# API / Service Ports
API_PORT=3000
ANALYZER_PORT=3001
RENDERER_PORT=3002
DESKTOP_PORT=3003

# External Vulnerability Feeds
OSV_API_URL=https://api.osv.dev
NVD_API_URL=https://services.nvd.nist.gov/rest/json
NVD_API_KEY=

# Security (MUST be overridden in any real deployment)
JWT_SECRET=CHANGE_ME_JWT_SECRET
ENCRYPTION_KEY=CHANGE_ME_32_CHAR_ENCRYPTION_KEY

# Logging
LOG_LEVEL=info
LOG_FILE=logs/shieldeye.log

# Optional single URL for tools that prefer it
DATABASE_URL=postgresql://shieldeye:CHANGE_ME_DB_PASSWORD@postgres:5432/shieldeye
